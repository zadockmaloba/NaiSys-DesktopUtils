//CASE WITH RANGE TEST

var int score = 23

call Core::Println : "============================"

match (score)
{
    case 80 ... 100 => call Core::Println : "Score: A"
    case 60 ... 79 => call Core::Println : "Score: B"
    case 50 ... 59 => call Core::Println : "Score: C"
    case 0 ... 49 => {
        call Core::Println : "Unclassed: "
        call Core::Println : score
    }
}